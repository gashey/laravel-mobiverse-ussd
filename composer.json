{
    "name": "gashey/mobiverse-ussd",
    "type": "library",
    "description": "Mobiverse USSD plugin for easy integration into Laravel projects",
    "keywords": [
        "php",
        "laravel",
        "mobiverse",
        "ussd",
        "plugin"
    ],
    "homepage": "https://github.com/gashey/mobiverse-ussd",
    "license": "MIT",
    "authors": [
        {
            "name": "George Kofi Hagan",
            "email": "george.hagan@mobivs.com"
        }
    ],
    "require": {
        "php": ">=7.1.3",
        "nesbot/carbon": "^2.18"
    },
    "require-dev": {
        "fzaninotto/faker": "~1.4",
        "phpunit/phpunit": "~5.0"
    },
    "autoload": {
        "psr-4": {
            "Gashey\\MobiverseUssd\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Gashey\\MobiverseUssd\\UssdServiceProvider"
            ]
        }
    }
}
